  body
    main
      .container

        nav 
          .nav-header
            h1 
              span.bright b
              | r
              span.bright u
              | s
              span.bright h
              | .
          .nav-menu
            i.far.fa-bell.fa-fw
            i.fas.fa-cog.fa-fw
            .user-profile
              p Welcome! 
                span John
              img(src="http://via.placeholder.com/40x40" alt="User Picture")

        .body 
          .dash-header
            ul
              li 
                a(href="#").active Dashboard
              li.dropdown
                a(href="#") Elements
                  i.fas.fa-caret-down.fa-fw
              li
                a(href="#") Charts
              li
                a(href="#") Calendar
              li
                a(href="#") Email
              li
                a(href="#") Chat
            .search
              span
                i.fas.fa-search.fa-fw
              input(type="text" placeholder="Search...")
          .dash-body
            .content(data-section="dashboard")
              .content-header
                h1 Dashboard
                .contacticons
                  i.fab.fa-twitter.fa-fw
                  i.fab.fa-instagram.fa-fw

              .content-body
                section.summary-section.grid-columns-4
                  .card
                    .card-header
                      i.fas.fa-users.fa-fw
                      h5 Users
                    .card-body
                      span 1,341
                  .card
                    .card-header
                      i.fas.fa-shopping-cart.fa-fw
                      h5 Purchases
                    .card-body
                      span 2,231,501       
                  .card
                    .card-header
                      i.fas.fa-american-sign-language-interpreting.fa-fw
                      h5 Agreements
                    .card-body    
                      span 60,134
                  .card
                    .card-header
                      i.fas.fa-lightbulb.fa-fw
                      h5 Ideas
                    .card-body
                      span 60

                section.revenueSection
                  .card.revenueChartCard
                    .card-header
                      h5 Vistor Report 
                    .card-body
                      .ct-chart
                    .card-footer
                  
                  .revenueSummary
                    .summarySection
                      .percentUp
                        i.fas.fa-caret-up.fa-fw
                        span 23%
                      div Total Visitors
                    .summarySection
                      .percentDown
                        i.fas.fa-caret-down.fa-fw
                        span 38%
                      div Bounce Rate

                section.orderSection
                  .orderSummaryContainer
                    table
                      thead
                        tr
                          th Product
                          th Status
                          th Price
                          th Order ID
                      tbody
                        tr
                          td iPhone 6S
                          td 
                            span.orderStatusShipped Shipped
                          td $603.59
                          td 8593402
                        tr
                          td Google Nexus
                          td 
                            span.orderStatusDelivered Delivered
                          td $450.31
                          td 310851
                        tr
                          td Galaxy S9
                          td 
                            span.orderStatusPending Pending
                          td $801.31
                          td 257900
                        tr
                          td Nokia Lumia
                          td
                            span.orderStatusCancelled Cancelled
                          td $235.51
                          td 423014
                        tr
                          td iPhone 5SE
                          td 
                            span.orderStatusBackOrder Back Order
                          td $330.51
                          td 938141

                section.mapSection.grid-columns-2
                  .card
                    .card-header
                      h5 Leaflet | OpenStreetMaps
                    .card-body
                      #map
                  .card
                    .card-header
                      h5 Leaflet | Stamen
                    .card-body
                      #mapstamen


                
                 
                  
